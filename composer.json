{
    "name": "yourusername/seo-dashboard",
    "description": "A PHP-based SEO dashboard application",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mysqli": "*",
        "dompdf/dompdf": "^3.1",
        "phpmailer/phpmailer": "^6.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "modules/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
